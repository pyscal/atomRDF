        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subtraction</rdfs:label>
    </owl:Class>
    


    <!-- http://purls.helmholtz-metadaten.de/asmo/ThermodynamicIntegration -->

